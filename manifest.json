{
  "name": "Overleaf Assistant",
  "version": "1.9.3",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "windows",
    "scripting"
  ],
  "host_permissions": [
    "https://www.overleaf.com/*"
  ],
  "background": {
    "service_worker": "sidepanel/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://www.overleaf.com/project/*"],
      "js": ["cache/api-logger-injector.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://www.overleaf.com/project/*"],
      "js": ["sidepanel/content.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://www.overleaf.com/latex/templates/*"],
      "js": ["template-button/template-to-extension.js"],
      "type": "module"
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Activate the extension"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/nbg-icon-16.png",
      "48": "icons/nbg-icon-48.png",
      "128": "icons/nbg-icon-128.png"
    }
  },
  "icons": {
    "16": "icons/nbg-icon-16.png",
    "48": "icons/nbg-icon-48.png",
    "128": "icons/nbg-icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "template-generation/overleafUploader.js",
        "icons/upload-icon-128.png",
        "icons/nbg-icon-128.png",
        "cache/api-logger-main-world.js"
      ],
      "matches": [
        "https://www.overleaf.com/*"
      ]
    }
  ]
}
